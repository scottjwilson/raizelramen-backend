{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "uuid": {
      "default": "",
      "type": "uuid"
    },
    "charge": {
      "type": "string"
    },
    "line_items": {
      "type": "json"
    },
    "products": {
      "collection": "product"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "inprogress", "completed"],
      "default": "pending"
    }
  }
}
